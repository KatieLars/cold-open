<h2>Sign In</h2>

<%= form_for :user do |f|%>
    <%= f.label :email %><br />
    <%= f.email_field :email %><br />

    <%= f.label :password %><br />
    <%= f.password_field :password %>

    <%= f.submit "Sign In" %>
<% end %></br>

<%= link_to "Sign in with Facebook", "/auth/facebook" %><br></br>
<%= link_to "Sign in with Google", "/auth/google" %><br></br>
