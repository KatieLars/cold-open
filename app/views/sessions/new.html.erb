<h2>Sign In</h2>

<%= form_for :user do |f|%>
  <div class="field">
    <%= f.label :username %><br />
    <%= f.email_field :username %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign In" %>
  </div>
<% end %></br>

<%= link_to image_tag("facebook_login_btn.jpeg", height: "44.4", width: "181.6"), "/auth/facebook" %><br></br>
<%= link_to image_tag("btn_google_signin_light_normal_web.png"), "/auth/google_oauth2" %><br></br>
